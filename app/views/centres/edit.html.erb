<h1>Edit centre</h1>


<%= form_for([@study, @centre]) do |f| %>
  <p>
    <%= f.label :centre_code %><br>
    <%= f.text_field :centre_code %>
  </p>
  <p>
    <%= f.label :centre_name %><br>
    <%= f.text_area :centre_name %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>


<%= link_to 'Back', study_path(@study) %>
